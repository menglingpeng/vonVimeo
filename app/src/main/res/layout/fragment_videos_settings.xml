<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/videos_settings_upload_defaults_rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/connected_apps_settings_to_other_accounts_rl_layout_marginBottom">

        <TextView
            android:id="@+id/videos_settings_upload_defaults_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/account_settings_general_title_tv_layout_marginTop"
            android:layout_marginBottom="@dimen/account_settings_general_title_tv_layout_marginBottom"
            android:textAppearance="@style/TextAppearance.AppCompat.Title"
            android:textSize="@dimen/videos_settings_upload_defaults_title_tv_textSize"
            android:text="@string/videos_settings_upload_defaults_title_tv_text"/>

        <RelativeLayout
            android:id="@+id/videos_settings_content_rating_default_rl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/videos_settings_upload_defaults_title_tv"
            android:layout_alignLeft="@+id/videos_settings_upload_defaults_title_tv">

            <TextView
                android:id="@+id/videos_settings_content_rating_default_title_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/connected_apps_settings_to_other_accounts_title_tv_layout_marginBottom"
                android:textAppearance="@style/Base.DialogWindowTitle.AppCompat"
                android:text="@string/videos_settings_content_rating_default_title_tv_text"/>

            <TextView
                android:id="@+id/videos_settings_content_rating_default_desc_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/marketing_settings_email_service_providers_desc_tv_layout_marginTop"
                android:layout_marginBottom="@dimen/connected_apps_settings_to_other_accounts_title_tv_layout_marginBottom"
                android:layout_below="@+id/videos_settings_content_rating_default_title_tv"
                android:layout_alignLeft="@+id/videos_settings_content_rating_default_title_tv"
                android:text="@string/videos_settings_content_rating_default_desc_tv_text"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/videos_settings_video_preferences_rl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/videos_settings_upload_defaults_title_tv"
            android:layout_alignLeft="@+id/videos_settings_upload_defaults_title_tv">

            <TextView
                android:id="@+id/videos_settings_video_preferences_title_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/connected_apps_settings_to_other_accounts_title_tv_layout_marginBottom"
                android:textAppearance="@style/Base.DialogWindowTitle.AppCompat"
                android:text="@string/videos_settings_video_preferences_title_tv_text"/>

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/videos_settings_privacy_rl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/videos_settings_upload_defaults_title_tv"
            android:layout_alignLeft="@+id/videos_settings_upload_defaults_title_tv">

            <TextView
                android:id="@+id/videos_settings_privacy_title_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/connected_apps_settings_to_other_accounts_title_tv_layout_marginBottom"
                android:textAppearance="@style/Base.DialogWindowTitle.AppCompat"
                android:text="@string/videos_settings_privacy_title_tv_text"/>

        </RelativeLayout>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/videos_settings_embed_presets_rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/videos_settings_upload_defaults_rl"
        android:layout_alignLeft="@+id/videos_settings_upload_defaults_rl"
        android:layout_marginBottom="@dimen/connected_apps_settings_to_other_accounts_rl_layout_marginBottom">

        <TextView
            android:id="@+id/videos_settings_embed_presets_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/account_settings_general_title_tv_layout_marginTop"
            android:layout_marginBottom="@dimen/account_settings_general_title_tv_layout_marginBottom"
            android:textAppearance="@style/TextAppearance.AppCompat.Title"
            android:textSize="@dimen/videos_settings_upload_defaults_title_tv_textSize"
            android:text="@string/videos_settings_embed_presets_title_tv_text"/>

        <TextView
            android:id="@+id/videos_settings_content_rating_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/connected_apps_settings_to_other_accounts_title_tv_layout_marginBottom"
            android:layout_below="@+id/videos_settings_embed_presets_title_tv"
            android:layout_alignLeft="@+id/videos_settings_embed_presets_title_tv"
            android:textAppearance="@style/Base.DialogWindowTitle.AppCompat"
            android:text="@string/marketing_settings_email_service_providers_title_tv_text"/>

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/videos_settings_player_logos_rl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/videos_settings_upload_defaults_rl"
        android:layout_alignLeft="@+id/videos_settings_upload_defaults_rl"
        android:layout_marginBottom="@dimen/connected_apps_settings_to_other_accounts_rl_layout_marginBottom">

        <TextView
            android:id="@+id/videos_settings_player_logos_title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/account_settings_general_title_tv_layout_marginTop"
            android:layout_marginBottom="@dimen/account_settings_general_title_tv_layout_marginBottom"
            android:textAppearance="@style/TextAppearance.AppCompat.Title"
            android:textSize="@dimen/videos_settings_upload_defaults_title_tv_textSize"
            android:text="@string/videos_settings_player_logos_title_tv_text"/>

        <TextView
            android:id="@+id/videos_settings_content__title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/connected_apps_settings_to_other_accounts_title_tv_layout_marginBottom"
            android:layout_below="@+id/videos_settings_player_logos_title_tv"
            android:layout_alignLeft="@+id/videos_settings_player_logos_title_tv"
            android:textAppearance="@style/Base.DialogWindowTitle.AppCompat"
            android:text="@string/marketing_settings_email_service_providers_title_tv_text"/>

    </RelativeLayout>

</RelativeLayout>